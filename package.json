    "license": "MIT",
    "dependencies": {
        "js-yaml": "^4.1.0",
        "nan": "^2.19.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.7.1",
        "@commitlint/config-conventional": "17.7.0",
        "husky": "8.0.3",
        "lint-staged": "14.0.1",
        "validate-branch-name": "^1.3.0"
    }
}
